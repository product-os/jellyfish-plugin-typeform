{
  "name": "@balena/jellyfish-plugin-typeform",
  "description": "Typeform Jellyfish Plugin",
  "version": "2.0.115",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-typeform.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint-fix",
      "git add ."
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint --typescript lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint-fix": "balena-lint --typescript --fix lib test",
    "unit": "jest test/unit",
    "test": "catch-uncommitted --skip-node-versionbot-changes && npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-plugin-base": "^2.1.88",
    "@balena/jellyfish-plugin-default": "^9.4.14",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.151",
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^10.0.3",
    "@balena/jellyfish-core": "^2.12.97",
    "@balena/jellyfish-plugin-channels": "^1.1.43",
    "@balena/jellyfish-test-harness": "^2.0.10",
    "@balena/jellyfish-types": "^0.5.29",
    "@balena/lint": "^5.4.2",
    "@types/jest": "^26.0.23",
    "catch-uncommitted": "^2.0.0",
    "depcheck": "^1.4.1",
    "deplint": "^1.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "simple-git-hooks": "^2.4.1",
    "ts-jest": "^26.5.6",
    "typedoc": "^0.20.36",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
