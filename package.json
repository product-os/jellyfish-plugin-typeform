{
  "name": "@balena/jellyfish-plugin-typeform",
  "description": "Typeform Jellyfish Plugin",
  "version": "3.0.62",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-typeform.git"
  },
  "engines": {
    "node": ">=12.15.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && npm run check && deplint && depcheck --ignore-bin-package --ignores=@types/jest",
    "lint-fix": "balena-lint --fix lib test",
    "unit": "jest test/unit",
    "integration": "jest ./test/integration/sync/typeform-translate.spec.ts",
    "test": "npm run lint && npm run unit",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "check": "jellycheck"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-plugin-base": "^2.1.151",
    "@balena/jellyfish-plugin-default": "^14.0.5",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.162",
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^13.0.6",
    "@balena/jellyfish-config": "^1.0.6",
    "@balena/jellyfish-core": "^3.0.0",
    "@balena/jellyfish-plugin-channels": "^1.1.136",
    "@balena/jellyfish-test-harness": "^3.0.47",
    "@balena/jellyfish-types": "^0.5.116",
    "@balena/lint": "^6.1.1",
    "@types/jest": "^26.0.23",
    "depcheck": "^1.4.1",
    "deplint": "^1.1.3",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "simple-git-hooks": "^2.4.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.20.36",
    "uuid": "^8.3.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }
}
