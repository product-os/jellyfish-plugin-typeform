{
  "name": "@balena/jellyfish-plugin-typeform",
  "description": "Typeform Jellyfish Plugin",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-typeform.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js lib/**/*.js > README.md",
    "postinstall": "if [ -z \"$CI\" ] && [ -d \".git\" ]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-plugin-base": "^1.1.6",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.103",
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^8.0.0",
    "@balena/jellyfish-core": "^1.4.27",
    "@balena/jellyfish-plugin-default": "4.0.0-move-typeform-to-own-plugin-104722053bfdcff96651625c5e28ed7aacd68d4b",
    "@balena/jellyfish-test-harness": "1.0.0-add-integration-test-harness-bbea8d6c0c9bac9d77e247c46ed32f105691bdd2",
    "@balena/lint": "^5.3.0",
    "ava": "^3.15.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.3.1",
    "deplint": "^1.1.3",
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.0.6",
    "lint-staged": "^10.5.3",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc"
    ]
  }
}
