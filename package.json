{
  "name": "@balena/jellyfish-plugin-typeform",
  "description": "Typeform Jellyfish Plugin",
  "version": "6.1.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-typeform.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "balena-lint --fix"
    ]
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "jest --bail --runInBand test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@balena/jellyfish-worker": "^24.2.2",
    "autumndb": "^20.0.1",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.6",
    "@balena/jellyfish-plugin-channels": "^2.0.116",
    "@balena/jellyfish-plugin-default": "^25.1.0",
    "@balena/jellyfish-plugin-product-os": "^6.0.1",
    "@balena/jellyfish-types": "^2.0.5",
    "@balena/lint": "^6.2.0",
    "@types/jest": "^27.5.0",
    "deplint": "^1.1.3",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.1",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^28.0.2",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-05-13T13:00:56.575Z"
  }
}
