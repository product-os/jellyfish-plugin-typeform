{
  "name": "@balena/jellyfish-plugin-typeform",
  "description": "Typeform Jellyfish Plugin",
  "version": "1.0.32",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-plugin-typeform.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "ava": {
    "timeout": "10m",
    "verbose": true,
    "serial": true,
    "failWithoutAssertions": true
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js lib/**/*.js > README.md",
    "postinstall": "if [ -z \"$CI\" ] && [ -d \".git\" ]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-plugin-base": "^1.2.30",
    "@balena/jellyfish-plugin-default": "^6.1.0",
    "bluebird": "^3.7.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@balena/ci-task-runner": "^0.2.109",
    "@balena/jellycheck": "^0.1.1",
    "@balena/jellyfish-action-library": "^8.0.67",
    "@balena/jellyfish-core": "^2.0.24",
    "@balena/jellyfish-plugin-channels": "^1.0.0",
    "@balena/jellyfish-test-harness": "^1.0.0",
    "@balena/lint": "^5.4.1",
    "ava": "^3.15.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.1.3",
    "jsdoc-to-markdown": "^7.0.0",
    "lint-staged": "^10.5.4",
    "uuid": "^8.3.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc"
    ]
  }
}
